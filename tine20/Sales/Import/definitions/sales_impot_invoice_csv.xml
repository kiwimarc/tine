<?xml version="1.0" encoding="UTF-8"?>
<config>
    <name>sales_import_invoice_csv</name>
    <model>Sales_Model_Invoice</model>
    <plugin>Sales_Import_Invoice_Csv</plugin>
    <type>import</type>
    <headline>1</headline>
    <dryrun>0</dryrun>
    <delimiter>,</delimiter>
    <extension>csv</extension>
    <mapping>
        <field>
            <source>description</source>
            <destination>description</destination>
        </field>
        <field>
            <source>date</source>
            <destination>date</destination>
        </field>
        <field>
            <source>credit_term</source>
            <destination>credit_term</destination>
        </field>
        <field>
            <source>price_net</source>
            <destination>price_net</destination>
        </field>
        <field>
            <source>sales_tax</source>
            <destination>sales_tax</destination>
        </field>
        <field>
            <source>cleared</source>
            <destination>cleared</destination>
        </field>
        <field>
            <source>type</source>
            <destination>type</destination>
        </field>
        <field>
            <source>start_date</source>
            <destination>start_date</destination>
        </field>
        <field>
            <source>end_date</source>
            <destination>end_date</destination>
        </field>
        <field>
            <source>contract</source>
            <destination>contract</destination>
        </field>
        <field>
            <source>costcenter</source>
            <destination>costcenter</destination>
        </field>
        <field>
            <source>customer</source>
            <destination>customer</destination>
        </field>
    </mapping>
</config>

